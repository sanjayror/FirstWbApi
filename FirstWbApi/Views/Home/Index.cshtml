   <body ng-app="myapp"> 
 
    <h4>Membership Record</h4>
   <div ng-controller="myctrl" align="center" >
        @*<div ng-controller="LoginCtrl" align="center">*@
            <table border="1">
                <tr bgcolor="gray">

                    <td> MemberID</td>
                    <td> Name</td>
                    <td>Birth</td>
                    <td>Gender</td>
                    <td>WorkoutType</td>
                    <td>Phone</td>
                    <td>Amount</td>
                    <td>JoiningDate</td>
