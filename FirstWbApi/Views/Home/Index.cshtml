                    <td>Email</td>
                    <td>Address</td>
                </tr>

                <tr ng-repeat="Item in Mem ">
                    <td>{{Item.MemberID}}</td>
                    <td><span>{{Item.Name}}</span></td>
                    <td><span>{{Item.Birth}}</span></td>
                    <td><span>{{Item.Gender}}</span></td>
                    <td><span>{{item.WorkoutType}}</span></td>
                    <td><span>{{Item.Phone}}</span></td>
                    <td><span>{{Item.Amount}}</span></td>
                    <td><span>{{Item.JoiningDate}}</span></td>
                    <td><span>{{item.Age}}</span></td>
                    <td><span>{{Item.Email}}</span></td>
                    <td><span>{{Item.Address}}</span></td>

                </tr>
            </table>
        </div>
</body>
</html>


